	$(NV_SOURCE)/multimedia/utils/nvosd \
	$(NV_SOURCE)/deepstream/sdk/include \
	$(NV_SOURCE)/deepstream/sdk/src/gst-utils/gstnvdshelper \
    

NV_COMPONENT_NEEDED_INTERFACE_DIRS   += \
        $(NV_SOURCE)/deepstream/sdk/src/utils/nvdsmeta \
        $(NV_SOURCE)/deepstream/sdk/src/gst-utils/gst-nvdsmeta

include $(NV_SOURCE)/tests-multimedia/nvgstapps/Makefile.common_gstreamer-1.0.tmk

ifeq ($(NV_TARGET_ARCH),l4t_64)
include $(NV_BUILD_DYNAMIC_EXECUTABLE)
endif

endif

