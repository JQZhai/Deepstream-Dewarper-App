ifdef NV_COMPONENT_FLAG_DYNAMIC_EXECUTABLE_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME                  := deepstream-dewarper-app
NV_COMPONENT_SOURCES               := \
	deepstream_dewarper_test.c

NV_COMPONENT_SYSTEMIMAGE_DIR := $(NV_SYSTEMIMAGE_DEEPSTREAM_DIR)

